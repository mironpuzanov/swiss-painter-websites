{
  "version": 2,
  "builds": [
    {
      "src": "**/*.html",
      "use": "@vercel/static"
    },
    {
      "src": "**/*.css",
      "use": "@vercel/static"
    },
    {
      "src": "**/*.js",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/malerbernhard$",
      "dest": "/Malergeschäft Bernhard AG/index.html"
    },
    {
      "src": "/malerbernhard/(.*)",
      "dest": "/Malergeschäft Bernhard AG/$1"
    },
    {
      "src": "/crncalo$",
      "dest": "/Crncalo Maler Gipsergeschäft/index.html"
    },
    {
      "src": "/crncalo/(.*)",
      "dest": "/Crncalo Maler Gipsergeschäft/$1"
    },
    {
      "src": "/deubelbeiss$",
      "dest": "/Maler Deubelbeiss GmbH/index.html"
    },
    {
      "src": "/deubelbeiss/(.*)",
      "dest": "/Maler Deubelbeiss GmbH/$1"
    },
    {
      "src": "/romang$",
      "dest": "/ROMANG Struktur & Farbe GmbH/index.html"
    },
