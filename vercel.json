    {
      "src": "/baudekoration$",
      "dest": "/Die Baudekoration GmbH/index.html"
    },
    {
      "src": "/baudekoration/(.*)",
      "dest": "/Die Baudekoration GmbH/$1"
    },
    {
      "src": "/beez$",
      "dest": "/Maler Beez AG/index.html"
    },
    {
      "src": "/beez/(.*)",
      "dest": "/Maler Beez AG/$1"
    },
    {
      "src": "/cantillo$",
      "dest": "/Cantillo Maler GmbH/index.html"
    },
    {
      "src": "/cantillo/(.*)",
      "dest": "/Cantillo Maler GmbH/$1"
