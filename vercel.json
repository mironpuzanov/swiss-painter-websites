      "src": "/indergand/(.*)",
      "dest": "/Maler Indergand/$1"
    },
    {
      "src": "/noser$",
      "dest": "/Maler Noser/index.html"
    },
    {
      "src": "/noser/(.*)",
      "dest": "/Maler Noser/$1"
    },
    {
      "src": "/decordesign$",
      "dest": "/Decor Design GmbH/index.html"
    },
    {
      "src": "/decordesign/(.*)",
      "dest": "/Decor Design GmbH/$1"
    },
    {
      "src": "/mbmaler$",
      "dest": "/MB Maler/index.html"
